{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.cppWithExt": true,
    },
    "clangd.arguments": [
        "--enable-config"
    ],
    "clangd.fallbackFlags": [
        "--target=x86_64-w64-windows-gnu",
        "-I${workspaceFolder}"
    ]
}